@using EntityLayer.Entities
@model List<Address>

<div class="w3l_header_left">
	@foreach (var item in Model)
	{
		<ul>
			<li><span class="glyphicon glyphicon-map-marker" aria-hidden="true"></span>@item.Adress</li>
			<li><span class="glyphicon glyphicon-earphone" aria-hidden="true"></span>@item.Phone</li>
			<li><span class="glyphicon glyphicon-envelope" aria-hidden="true"></span> <a href="mailto:@item.Mail">@item.Mail</a></li>
		</ul>
	}
</div>